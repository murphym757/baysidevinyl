<p id="notice"><%= notice %></p>

<h1>Tracks</h1>

<table>
  <thead>
    <tr>
      <th>Artist</th>
      <th>Album</th>
      <th>Song</th>
      <th>Genre</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <% @tracks.each do |track| %>
  <div class="col-xs-6 col-md-3">
    <div class="thumbnail">
      <%= link_to (image_tag track.image.url(:medium), class: 'image'), track %>
    </div>
  </div>
<% end %>

<br>

<%= link_to 'New Track', new_track_path %>
